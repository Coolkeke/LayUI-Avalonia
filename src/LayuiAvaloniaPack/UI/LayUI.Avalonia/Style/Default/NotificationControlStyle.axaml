<Styles
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:lay="clr-namespace:LayUI.Avalonia.Controls"
    xmlns:layCv="clr-namespace:LayUI.Avalonia.Converters"
    xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    <Design.PreviewWith>
        <Border Padding="20">
            <StackPanel>
                <lay:LayNotificationControl Width="150" IsOpen="true" />
                <lay:LayNotificationControl Width="150" Type="Success" />
                <lay:LayNotificationControl Width="150" Type="Warning" />
                <lay:LayNotificationControl Width="150" Type="Error" />
            </StackPanel>
        </Border>
    </Design.PreviewWith>
    <Style Selector="lay|LayNotificationControl">
        <Setter Property="MinWidth" Value="240" />
        <Setter Property="Background" Value="White" />
        <Setter Property="ClipToBounds" Value="false" />
        <Setter Property="HorizontalAlignment" Value="Right" />
        <Setter Property="Margin" Value="10 5" />
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="CornerRadius" Value="2" />
        <Setter Property="Width" Value="300" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <lay:LayCard
                        Name="PART_BorderBody"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        ClipToBounds="{TemplateBinding ClipToBounds}"
                        CornerRadius="{TemplateBinding CornerRadius}"
                        Opacity="0">
                        <lay:LayCard.Transitions>
                            <Transitions>
                                <DoubleTransition Property="Opacity" Duration="0:0:0.2" />
                                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.2" />
                            </Transitions>
                        </lay:LayCard.Transitions>
                        <Grid ColumnDefinitions="auto,*,auto">
                            <Path
                                Name="PART_Icon"
                                Width="25"
                                Height="25"
                                Margin="5,14"
                                VerticalAlignment="Top"
                                Stretch="Fill" />
                            <Grid Grid.Column="1" Margin="5,10">
                                <Grid RowDefinitions="auto,auto">
                                    <TextBlock FontSize="18" Text="{Binding Title, FallbackValue=这是标题}" />
                                    <TextBlock
                                        Grid.Row="1"
                                        FontSize="15"
                                        Text="{Binding Message, FallbackValue=这是信息这是信息这是信息}"
                                        TextTrimming="WordEllipsis"
                                        TextWrapping="Wrap" />
                                </Grid>
                            </Grid>
                            <Button
                                Name="PART_CloseButton"
                                Grid.Column="2"
                                VerticalAlignment="Top">
                                <Button.Template>
                                    <ControlTemplate>
                                        <Border
                                            Width="20"
                                            Height="20"
                                            Background="{DynamicResource Danger}">
                                            <Path
                                                Margin="5"
                                                Data="M550.848 502.496l308.64-308.896a31.968 31.968 0 1 0-45.248-45.248l-308.608 308.896-308.64-308.928a31.968 31.968 0 1 0-45.248 45.248l308.64 308.896-308.64 308.896a31.968 31.968 0 1 0 45.248 45.248l308.64-308.896 308.608 308.896a31.968 31.968 0 1 0 45.248-45.248l-308.64-308.864z"
                                                Fill="White"
                                                Stretch="Fill" />
                                        </Border>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>
                        </Grid>
                    </lay:LayCard>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
