<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--  辅助文字  -->
    <sys:Double x:Key="Extra Small">12</sys:Double>
    <!--  正文（小）  -->
    <sys:Double x:Key="Small">13</sys:Double>
    <!--  正文  -->
    <sys:Double x:Key="Base">14</sys:Double>
    <!--  小标题  -->
    <sys:Double x:Key="Medium">16</sys:Double>
    <!--  标题  -->
    <sys:Double x:Key="Large">18</sys:Double>
    <!--  主标题  -->
    <sys:Double x:Key="Extra Large">20</sys:Double>
</ResourceDictionary>
