                        <Grid Width="150" Height="150">
                            <UniformGrid
                                Width="100"
                                Height="50"
                                Columns="5">
                                <Grid
                                    Name="PART_Grid1"
                                    Margin="1,0"
                                    Background="{TemplateBinding Background}" />
                                <Grid
                                    Name="PART_Grid2"
                                    Margin="1,0"
                                    Background="{TemplateBinding Background}" />
                                <Grid
                                    Name="PART_Grid3"
                                    Margin="1,0"
