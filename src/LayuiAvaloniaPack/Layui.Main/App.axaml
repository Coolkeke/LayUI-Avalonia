<prism:PrismApplication
    x:Class="Layui.Main.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Layui.Main"
    xmlns:prism="http://prismlibrary.com/"
    RequestedThemeVariant="Default">
    <!--  "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options.  -->

    <Application.Resources>
        <ResourceInclude x:Key="zh_CN" Source="/Languages/zh_CN.axaml" />
        <ResourceInclude x:Key="en_US" Source="/Languages/en_US.axaml" />
    </Application.Resources>
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://LayUI.Avalonia/Themes/Default.axaml" />
    </Application.Styles>
</prism:PrismApplication>                                                             